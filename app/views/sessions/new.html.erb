<div class="logsign_cont">
    <div id="spacer">
    </div>
    <div id="logo_container">
        <img src="logo.png" id="logo"/>
    </div>
    <h1>login</h1>
    <%= form_for(:session, url: login_path) do |f| %>
        <table id="login">
            <tr>
                <td>
                    No Account?
                </td>
                <td>
                     <%= link_to "Sign Up", signup_path %>
                </td>
            </tr>
            <tr>
                <td>
                    <%= f.label :email %>
                </td>
                <td>
                    <%= f.email_field :email %>
                </td>
            </tr>
            <tr>
                <td>
                    <%= f.label :password %>
                </td>
                <td>
                    <%= f.password_field :password %>
                </td>
            </tr>
            <tr>
                <td>
                    Remember Me
                </td>
                <td>
                    <%= f.label :remember_me do %>
                        <%= f.check_box :remember_me %>
                    <% end %>
                </td>
            </tr>
            <tr align="center" id="buttonbox">
                <td colspan="2">
                    <%= f.submit "Login", :class => 'reg_button'  %>
                </td>
            </tr>
        </table>
    <% end %>
</div>
<div class="right">
    <div id="spacer">
    </div>
    Placeholder div
</div>